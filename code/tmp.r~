geninegicycle <- function(emm){
    library(plyr)
    tmp <- data.frame(emm=emm)
    tmp$edo <- sub("^([a-z]+)[-][0-9]+", "\\1", emm)
    tmp$edon <- mapvalues(x=tmp$edo, from = c("ags", "bc", "bcs", "cam", "coa", "col", "cps", "cua", "df", "dgo", "gua", "gue", "hgo", "jal", "mex", "mic", "mor", "nay", "nl", "oax", "pue", "que", "qui", "san", "sin", "son", "tab", "tam", "tla", "ver", "yuc", "zac"), to = 1:32)
    
